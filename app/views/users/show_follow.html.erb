<div class="row">
  <% provide(:title, @title) %>

  <div class="large-12 columns">
    <h3><%= @title %></h3>
    <% if @users.any? %>
        <ul class="small-block-grid-2 medium-block-grid-3 large-block-grid-3">
          <%= render @users %>
        </ul>
        <%= will_paginate %>
    <% end %>
  </div>
</div>




